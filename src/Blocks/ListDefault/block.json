{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "shp/gantrisch-adb-offer-list",
	"title": "ADB List",
	"category": "media",
	"icon": "format-image",
	"description": "A list view of all matching offer entries. Optionally filtered by category.",
	"keywords": ["offer", "angebot", "list"],
	"textdomain": "shp_gantrisch_adb",
	"attributes": {
		"align": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		},
		"button_text": {
			"type": "string"
		},
		"image_size": {
			"type": "string",
			"default": "small"
		},
		"initial_count": {
			"type": "number",
			"default": 12
		},
		"load_more_text": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"category": {
			"type": "string"
		}
	},
	"supports": {
		"align": ["wide", "full"],
		"html": false
	},
	"editorStyle": "file:./assets/dist/styles/editor/index.min.css",
	"editorScript": "file:./assets/dist/scripts/editor.js",
	"viewScript": "file:./assets/dist/scripts/viewScript.js",
	"style": [
		"file:./assets/dist/styles/shared/index.min.css",
		"shp-gantrisch-adb-offer-list-shared"
	]
}
