		"title_equipmentrental": {
			"type": "string",
			"default": "Materialmiete"
		},
		"title_safety": {
			"type": "string",
			"default": "Sicherheitshinweise"
		},
		"title_signals": {
			"type": "string",
			"default": "Signalisation"
		}
